{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "./node_modules/.bin/cdk"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --runInBand --detectOpenHandles",
    "test:dev": "jest --runInBand --detectOpenHandles --watch",
    "lint": "eslint lib/** bin/** --ext .ts",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "diff": "cdk diff --path-metadata false --version-reporting false",
    "synth": "cdk synth --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^6.0.1",
    "@types/jest": "^29.5.3",
    "eslint": "^8.46.0",
    "@types/node": "20.4.6",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6",
    "@guardian/cdk": "50.10.6",
    "aws-cdk": "2.77.0",
    "aws-cdk-lib": "2.77.0",
    "constructs": "10.2.8"
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  }
}
