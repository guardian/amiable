{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "./node_modules/.bin/cdk"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --runInBand --detectOpenHandles",
    "test:dev": "jest --runInBand --detectOpenHandles --watch",
    "lint": "eslint lib/** bin/** --ext .ts",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "diff": "cdk diff --path-metadata false --version-reporting false",
    "synth": "cdk synth --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.147.0",
    "@guardian/eslint-config-typescript": "1.0.1",
    "@guardian/private-infrastructure-config": "git+ssh://git@github.com:guardian/private-infrastructure-config.git#1.2.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "aws-cdk": "1.147.0",
    "eslint": "8.10.0",
    "jest": "27.5.1",
    "jest-teamcity-reporter": "^0.9.0",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "@aws-cdk/aws-autoscaling": "1.147.0",
    "@aws-cdk/aws-ec2": "1.147.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "1.147.0",
    "@aws-cdk/aws-iam": "1.147.0",
    "@aws-cdk/aws-rds": "1.147.0",
    "@aws-cdk/core": "1.147.0",
    "@guardian/cdk": "38.0.0-beta.1",
    "source-map-support": "^0.5.16"
  }
}
